CREATE TABLE students (
    student_id NUMBER PRIMARY KEY,
    first_name VARCHAR2(50),
    last_name VARCHAR2(50),
    age NUMBER,
    department VARCHAR2(50)
);
INSERT INTO students (student_id, first_name, last_name, age, department)
